{
  "name": "square1-web",
  "version": "0.0.1",
  "description": "Front end for the square1 lead tracking and resource management system.",
  "scripts": {
    "api": "nodemon tools/apiServer.js --exec babel-node",
    "start": "babel-node tools/server.js",
    "lint": "esw webpack.config.* src tools --color || exit 0",
    "lint:watch": "npm run lint --watch",
    "test": "find ./src -name '*.test.js' | xargs mocha --opts ./mocha.opts || exit 0",
    "test:watch": "npm run test -- --watch",
    "prebuild": "rm -rf client && mkdir client",
    "build": "NODE_ENV=production babel-node tools/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wolfspace/square1-web"
  },
  "author": "Mike Rourke",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/wolfspace/square1-web/issues"
  },
  "homepage": "https://github.com/wolfspace/square1-web#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "config": "^1.24.0",
    "immutable": "^3.8.1",
    "material-ui": "^0.16.6",
    "material-ui-datatables": "^0.16.4",
    "normalizr": "^3.1.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-google-maps-loader": "^2.0.2",
    "react-google-places-suggest": "^2.0.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-axios-middleware": "^3.0.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-alias": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "json-server": "^0.9.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-stats-plugin": "^0.1.4",
    "webpack-visualizer-plugin": "^0.1.10"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  }
}
