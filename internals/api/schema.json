{
  "leads": {
    "type": "array",
    "minItems": 100,
    "maxItems": 150,
    "items": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "minimum": 0,
          "exclusiveMinimum": true
        },
        "leadName": {
          "type": "string",
          "faker": "name.findName"
        },
        "contactName": {
          "type": "string",
          "faker": "name.findName"
        },
        "source": {
          "type": "string",
          "pattern": "Did work in area|Facebook|Flyer|Home Advisor|Saw Sign"
        },
        "leadFee": {
          "type": "integer",
          "faker": "finance.amount"
        },
        "phone": {
          "type": "string",
          "chance": {
            "phone": {
              "country": "us"
            }
          },
          "pattern": "phoneNumber"
        },
        "email": {
          "type": "string",
          "format": "email",
          "faker": "internet.email"
        },
        "address": {
          "type": "string",
          "faker": "address.streetAddress"
        },
        "lat": {
          "type": "number",
          "faker": "address.latitude"
        },
        "lng": {
          "type": "number",
          "faker": "address.longitude"
        },
        "description": {
          "type": "string",
          "chance": {
            "sentence": {
              "words": 10
            }
          }
        },
        "status": {
          "type": "string",
          "pattern": "New|Selling|Won|Qualified|Lost"
        },
        "assignTo": {
          "type": "string",
          "pattern": "Scott|Chuckles|Biscuits"
        },
        "notes": {
          "type": "array",
          "minItems": 5,
          "maxItems": 10,
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer",
                "minimum": 0,
                "exclusiveMinimum": true
              },
              "contents": {
                "type": "string",
                "chance": {
                  "sentence": {
                    "words": 10
                  }
                }
              },
              "isPrivate": {
                "type": "boolean"
              },
              "createdBy": {
                "type": "string",
                "pattern": "mike|lis|joe|josh"
              },
              "createdAt": {
                "type": "number",
                "chance": "timestamp",
                "minimum": 1451606400
              },
              "updatedAt": {
                "type": "number",
                "chance": "timestamp",
                "minimum": 1451606400
              }
            },
            "required": [
              "id",
              "contents",
              "isPrivate",
              "createdBy",
              "createdAt",
              "updatedAt"
            ]
          }
        },
        "changes": {
          "type": "array",
          "minItems": 5,
          "maxItems": 10,
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer",
                "minimum": 0,
                "exclusiveMinimum": true
              },
              "changeType": {
                "type": "string",
                "pattern": "create|update"
              },
              "iconName": {
                "type": "string",
                "pattern": "add_circle_outline|contact_mail|contact_phone"
              },
              "title": {
                "type": "string",
                "chance": {
                  "sentence": {
                    "words": 5
                  }
                }
              },
              "details": {
                "type": "string",
                "chance": {
                  "sentence": {
                    "words": 10
                  }
                }
              },
              "createdBy": {
                "type": "string",
                "pattern": "mike|lis|joe|josh"
              },
              "createdAt": {
                "type": "number",
                "chance": "timestamp",
                "minimum": 1451606400
              },
              "updatedAt": {
                "type": "number",
                "chance": "timestamp",
                "minimum": 1451606400
              }
            },
            "required": [
              "id",
              "changeType",
              "iconName",
              "title",
              "details",
              "createdBy",
              "createdAt",
              "updatedAt"
            ]
          }
        },
        "messages": {
          "type": "array",
          "minItems": 5,
          "maxItems": 10,
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer",
                "minimum": 0,
                "exclusiveMinimum": true
              },
              "messageType": {
                "type": "string",
                "pattern": "text|email"
              },
              "recipient": {
                "type": "string",
                "chance": {
                  "phone": {
                    "country": "us"
                  }
                },
                "pattern": "phoneNumber"
              },
              "subject": {
                "type": "string",
                "chance": {
                  "sentence": {
                    "words": 5
                  }
                }
              },
              "body": {
                "type": "string",
                "chance": {
                  "sentence": {
                    "words": 10
                  }
                }
              },
              "createdBy": {
                "type": "string",
                "pattern": "mike|lis|joe|josh"
              },
              "createdAt": {
                "type": "number",
                "chance": "timestamp",
                "minimum": 1451606400
              },
              "updatedAt": {
                "type": "number",
                "chance": "timestamp",
                "minimum": 1451606400
              }
            },
            "required": [
              "id",
              "messageType",
              "recipient",
              "subject",
              "body",
              "createdBy",
              "createdAt",
              "updatedAt"
            ]
          }
        },
        "createdBy": {
          "type": "string",
          "pattern": "mike|lis|joe|josh"
        },
        "createdAt": {
          "type": "number",
          "chance": "timestamp",
          "minimum": 1451606400
        },
        "updatedAt": {
          "type": "number",
          "chance": "timestamp",
          "minimum": 1451606400
        }
      },
      "required": [
        "id",
        "leadName",
        "contactName",
        "source",
        "leadFee",
        "phone",
        "email",
        "address",
        "lat",
        "lng",
        "description",
        "status",
        "assignTo",
        "notes",
        "changes",
        "messages",
        "createdBy",
        "createdAt",
        "updatedAt"
      ]
    }
  }
}
