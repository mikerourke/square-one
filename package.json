{
  "name": "square-one",
  "version": "0.0.1",
  "description": "Front end for the Square One lead tracking and resource management system.",
  "scripts": {
    "api": "cross-env USE_MOCK_API=true node internals/api/server.js",
    "api:watch": "nodemon internals/api/server.js",
    "prebuild": "rm -rf client && mkdir client",
    "build": "cross-env NODE_ENV=production node internals/scripts/build.js",
    "build:cleanup": "rm -rf client && rm stats.json || exit 0",
    "clean-install": "rm -rf node_modules && yarn cache clean && yarn install",
    "flow": "flow check || exit 0",
    "generate-data": "node internals/api/generator.js",
    "lint": "esw webpack.config.* src tools --color || exit 0",
    "lint:watch": "npm run lint --watch",
    "server": "cross-env NODE_ENV=development node internals/server",
    "setup": "npm run generate-data && npm install",
    "start:mock": "concurrently --kill-others \"npm run api\" \"npm run server\"",
    "start:prod": "cross-env USE_MOCK_API=false npm run server",
    "start:watch": "concurrently --kill-others \"npm run api:watch\" \"npm run server\"",
    "test": "cross-env NODE_ENV=test jest || exit 0",
    "test:watch": "npm run test -- --watch",
    "torus:start": "torus run npm run start",
    "torus:test": "torus run npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikerourke/square-one"
  },
  "author": "Mike Rourke",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/mikerourke/square-one/issues"
  },
  "homepage": "https://github.com/mikerourke/square-one#readme",
  "dependencies": {
    "axios": "^0.16.0",
    "glamor": "^2.20.24",
    "glamorous": "^3.13.1",
    "google-maps": "^3.2.1",
    "immutable": "^3.8.1",
    "material-ui": "^0.18.0",
    "moment": "^2.17.1",
    "normalizr": "^3.2.0",
    "phones": "^2.0.1",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-axios-middleware": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.7.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "chalk": "^1.1.3",
    "compression-webpack-plugin": "^0.4.0",
    "concurrently": "^3.2.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^3.0.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.0",
    "flow-bin": "^0.45.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^19.0.0",
    "json-loader": "^0.5.4",
    "json-schema-faker": "0.4.1",
    "json-server": "^0.10.0",
    "jsonfile": "^3.0.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2",
    "redux-mock-store": "^1.2.2",
    "shelljs": "^0.7.6",
    "style-loader": "^0.17.0",
    "twilio": "^3.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/setup.js",
    "roots": [
      "<rootDir>/src"
    ],
    "verbose": false
  }
}
