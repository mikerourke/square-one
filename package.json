{
  "name": "square1-web",
  "version": "0.0.1",
  "description": "Front end for the square1 lead tracking and resource management system.",
  "scripts": {
    "api": "node internals/api/server.js",
    "api:watch": "nodemon internals/api/server.js",
    "prebuild": "rm -rf client && mkdir client",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --json > stats.json  || exit 0",
    "predoc": "rm -rf docs",
    "doc": "esdoc -c esdoc.json || exit 0",
    "flow": "flow check || exit 0",
    "lint": "esw webpack.config.* src tools --color || exit 0",
    "lint:watch": "npm run lint --watch",
    "server": "cross-env NODE_ENV=development node internals/server",
    "setup": "rm -rf node_modules && yarn cache clean && yarn install",
    "start": "concurrently --kill-others \"npm run api\" \"npm run server\"",
    "test": "cross-env NODE_ENV=test jest || exit 0",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wolfspace/square1-web"
  },
  "author": "Mike Rourke",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/wolfspace/square1-web/issues"
  },
  "homepage": "https://github.com/wolfspace/square1-web#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "google-maps": "^3.2.1",
    "immutable": "^3.8.1",
    "material-ui": "^0.16.6",
    "material-ui-datatables": "^0.16.4",
    "normalizr": "^3.2.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-axios-middleware": "^3.0.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.7.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "chalk": "^1.1.3",
    "concurrently": "^3.2.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "esdoc": "^0.5.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "beta",
    "file-loader": "^0.10.0",
    "flow-bin": "^0.39.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^18.1.0",
    "json-loader": "^0.5.4",
    "json-server": "^0.9.4",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2",
    "redux-mock-store": "^1.2.2",
    "shelljs": "^0.7.6",
    "style-loader": "^0.13.1",
    "styled-components": "^1.4.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/setup.js",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testRegex": "test/.*\\.test\\.js$"
  }
}
