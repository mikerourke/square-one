{
  "name": "square1-web",
  "version": "0.0.1",
  "description": "Front end for the square1 lead tracking and resource management system.",
  "scripts": {
    "api": "node_modules/.bin/json-server --watch db.json --port 8082",
    "start": "babel-node tools/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint --watch",
    "build:remove-folder": "node_modules/.bin/rimraf ./client",
    "build:prepare-folder": "npm run build:remove-folder && mkdir client",
    "prebuild": "npm-run-all build:prepare-folder",
    "build:run": "babel-node tools/build.js",
    "build": "npm run build:prepare-folder && npm run build:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wolfspace/square1-web"
  },
  "author": "Mike Rourke",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/wolfspace/square1-web/issues"
  },
  "homepage": "https://github.com/wolfspace/square1-web#readme",
  "dependencies": {
    "material-ui": "^0.16.6",
    "material-ui-datatables": "^0.16.4",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-google-maps-loader": "^2.0.2",
    "react-google-places-suggest": "^2.0.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "superagent": "^3.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.0",
    "eslint-import-resolver-alias": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.14",
    "file-loader": "^0.9.0",
    "json-server": "^0.9.4",
    "npm-run-all": "^3.1.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
